{
  "name": "eslint-get-rules",
  "version": "0.0.1",
  "description": "Tool for getting meta-data of eslint rules in the current project.",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "dev": "preppy --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "build": "preppy",
    "release": "git push && release-it --ci patch",
    "release:minor": "git push && release-it --ci minor",
    "release:major": "git push && release-it --ci major"
  },
  "files": [
    "lib/"
  ],
  "author": {
    "name": "Sebastian Werner",
    "email": "s.werner@sebastian-software.de",
    "url": "http://sebastian-software.de/werner"
  },
  "license": "Apache-2.0",
  "keywords": [
    "eslint",
    "util"
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "babel-jest": "^27.4.6",
    "eslint": "^8.7.0",
    "eslint-plugin-jsdoc": "^37.7.0",
    "jest": "^27.4.7",
    "preppy": "^11.0.2",
    "prettier": "^2.5.1",
    "release-it": "^14.12.4",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@eslint/eslintrc": "^1.0.5",
    "core-js": "^3.20.3",
    "pkg-dir": "^5.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.7.0"
  }
}
